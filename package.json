    "@testing-library/react": "^12.1.2",
    "@types/node": "^17.0.10",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.2",
    "codeceptjs": "^3.2.3",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.2.2",
    "playwright": "^1.18.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.15",
    "ts-node": "^10.4.0",
    "typescript": "4.5.5"
